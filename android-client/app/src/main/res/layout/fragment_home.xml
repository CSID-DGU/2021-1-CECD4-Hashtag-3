<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:tools="http://schemas.android.com/tools"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             tools:context=".HomeFragment" android:background="#FEF2C7">

    <Button
            android:fontFamily="@font/enanum"
            android:text="로그인"
            android:textSize="23sp"
            android:layout_marginTop="100dp"

            android:layout_width="266dp"
            android:layout_height="66dp"
            android:layout_gravity="center"
            android:id="@+id/login_btn" android:textColor="#5D3D37" android:background="@drawable/button_brown"/>
    <Button
            android:background="@drawable/button_background"
            android:fontFamily="@font/enanum"
            android:text="회원가입"
            android:textSize="23sp"
            android:layout_marginLeft="3dp"
            android:layout_width="266dp"
            android:layout_marginTop="180dp"
            android:layout_gravity="center"
            android:layout_above="@id/login_btn"
            android:layout_height="66dp" android:id="@+id/register_btn" android:textColor="@color/white"/>
    <TextView
            android:textColor="@color/black"
            android:fontFamily="@font/enanum"
            android:layout_width="296dp"
            android:layout_height="226dp" android:id="@+id/start_title" android:layout_gravity="center"
            android:text="어서오세요!\n 환영합니다." android:textSize="23sp" android:textStyle="bold"
            android:textAlignment="center"/>

</FrameLayout>